  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "5.18.2",
    "crypto-js": "4.1.1",
    "ethers": "^5.6.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.1.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@vercel/ncc": "^0.33.3",
    "axios": "^0.26.1",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
