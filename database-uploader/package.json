{
  "name": "database-uploader",
  "version": "0.2.1",
  "description": "This is a component of Adapter.js which allows users to upload cached data to the external adapter's database where it is encrypted using public key cryptography so it can only be decrypted by the Chainlink node using the 'external-adapter-entry' component.",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/app.js",
    "dist": "ncc build -m ./src/index.ts -o dist",
    "build": "tsc",
    "test": "jest"
  },
